{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/posts/",
      "dest": "index.js"
    },
    {
      "src": "/posts/(.*)",
      "headers": { "Location": "/posts/$1" },
      "dest": "index.js"
    },
    {
      "src": "/users",
      "dest": "app.js"
    },
    {
      "src": "/users/(.*)",
      "headers": { "Location": "/users/$1" },
      "dest": "index.js"
    },
    {
      "src": "/users/(.*)/tasks",
      "headers": { "Location": "/users/$1/tasks" },
      "dest": "index.js"
    },
  ]
}