{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/posts/",
      "dest": "index.js"
    },
    {
      "src": "/api/v1/posts/(.*)",
      "headers": { "Location": "/api/v1/posts/$1" },
      "dest": "index.js"
    },
    {
      "src": "/api/v1/users",
      "dest": "index.js"
    },
    {
      "src": "/api/v1/users/(.*)",
      "headers": { "Location": "/api/v1/users/$1" },
      "dest": "index.js"
    },
    {
      "src": "/api/v1/users/(.*)/tasks",
      "headers": { "Location": "/api/v1/users/$1/tasks" },
      "dest": "index.js"
    },
  ]
}