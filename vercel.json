{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "index.ts" },
    { "src": "/api/v1/items", "dest": "index.ts" },
    {
      "src": "/api/v1/items/(.*)",
      "headers": { "Location": "/api/v1/items/$1" },
      "dest": "index.ts"
    },
    { "src": "/api/v1/posts", "dest": "index.ts" }
  ]
}